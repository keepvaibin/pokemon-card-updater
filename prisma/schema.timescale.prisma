generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/ts-client"
}

datasource db {
  provider = "postgresql"
  url      = env("TIMESCALE_URL")
}

model PriceHistory {
  cardId            String
  time              DateTime
  averageSellPrice  Float?
  source            String?

  @@id([cardId, time])
  @@index([time(sort: Desc)])
  @@map("PriceHistory")
}
